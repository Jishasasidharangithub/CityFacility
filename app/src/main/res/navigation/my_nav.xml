    <fragment
        android:id="@+id/bookingDetailsFragment"
        android:name="com.example.cityfacility.ui.fragment.BookingDetailsFragment"
        android:label="fragment_booking_details"
        tools:layout="@layout/fragment_booking_details" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.cityfacility.ui.fragment.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />

</navigation>